{
    "name": "miladimos/laravel-filemanager",
    "description": "a powerful, flexible laravel file manager",
    "homepage": "https://github.com/miladimos/laravel-filemanager",
    "type": "library",
    "keywords": [
        "laravel",
        "laravel-filemanager",
        "laravel support",
        "lumen packages",
        "laravel packages",
        "lumen support",
        "file uploader",
        "uploader",
        "file manager"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com",
            "role": "maintaner",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Miladimos\\FileManager\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miladimos\\FileManager\\Providers\\FileManagerServiceProvider"
            ],
            "aliases": {
                "FileManager": "Miladimos\\FileManager\\Facades\\FileManagerFacade"
            }
        }
    },
    "require": {
        "php": ">=7.4|^8",
        "nesbot/carbon": "^2.50.0",
        "morilog/jalali": ">=v3.1.0",
        "intervention/image": "^2.6",
        "ramsey/uuid": "^4.1",
        "webpatser/laravel-uuid": "^4",
        "league/mime-type-detection": "^1.7"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
